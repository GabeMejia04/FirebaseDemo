<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="aydin.firebasedemo.WelcomeController">
    <children>
        <VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
            <padding>
                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
            </padding>
            <children>
                <Button fx:id="registerButton" onAction="#handleRegister" text="Register" prefHeight="40.0" prefWidth="150.0" />
                <Button fx:id="signInButton" onAction="#handleSignIn" text="Sign In" prefHeight="40.0" prefWidth="150.0" />
            </children>
        </VBox>
    </children>
</AnchorPane>
